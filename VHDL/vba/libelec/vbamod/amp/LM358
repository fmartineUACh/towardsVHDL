.PARSET LM358 _OPAMPPNPD
+ VNOISE     = 30     ; spectral noise density in [nV/sqrt(Hz)]
+ GBW        = 1MEG   ; gain-bandwidth product
+ TN         = 25     ; nominal temperature in [C]
+ TMIN       = 0      ; minimum temperature of operation (simulation) in [C]
+ TMAX       = 70     ; maximum temperature of operation (simulation) in [C]
+ SRNN       = 0.5    ; negative going slew rate for nominal temperature in [V/us]
+ SRNMIN     = 0.5    ; negative going slew rate for min temperature in [V/us]
+ SRNMAX     = 0.5    ; negative going slew rate for max temperature in [V/us]
+ SRP        = 0.48   ; positive going slew rate in [V/us]
+ VCC        = 5      ; typical positive supply voltage
+ VIN_CM_MAX = 3.5    ; higher limit of input common mode voltage
+ VEE        = 0      ; typical negative supply voltage
+ VIN_CM_MIN = 0      ; lower limit of input common mode voltage
+ IBIAS_N    = 45E-9  ; input bias current for nominal temperature
+ IBIAS_MIN  = 45E-9  ; input bias current for minimum temperature
+ ROUT       = 200    ; output resistance
+ ROAC       = 70     ; output resistance for alternating current
+ AVD        = 500K   ; open loop voltage gain in [V/V]
+ IOSN       = 3E-9   ; input offset current for nominal temperature
+ IOSMIN     = 4E-9   ; input offset current for minimum temperature
+ IOSMAX     = 2E-9   ; input offset current for maximum temperature
+ VOSN       = .002   ; input offset voltage for nominal temperature
+ VOSMIN     = .0022  ; input offset voltage for minimum temperature
+ VOSMAX     = .0017  ; input offset voltage for maximum temperature
+ CMRR       = 100    ; common mode rejection ratio in [dB]
+ FCMRR      = 300000 ; 3dB frequency of common mode rejection ratio
+ PSRR       = 100    ; average power supply rejection ratio in [dB]
+ FPSRRP     = 100    ; 3dB frequency of positive power supply rejection ratio
+ FPSRRN     = 8000   ; 3dB frequency of negative power supply rejection ratio
+ ISUP       = .00036 ; supply current
+ ISUP0      = .0002  ; extrapolated supply current for VCC=0
+ OVERSHOOT  = 20     ; overshoot of small signal pulse response in []
+ PHIM       = 40     ; phase margin for 0dB frequency in [deg]
+ PHIM_OVRSHT= 0      ; phase margin/overshoot switch [1/0]
+ ISCP       = 0.04   ; output positive short-circuit current
+ ISCN       = 0.03   ; output negative short-circuit current
+ VOUT_MIN   = 0.008  ; minimum output voltage
+ VOUT_MAX   = 3.5    ; maximum output voltage
