.PARSET LM741 _LM741
+ VNOISE     = 20    STAT 15    30     :GAUSS  ; spectral noise density in [nV/sqrt(Hz)]
+ GBW        = 1MEG  STAT 700K  1.5MEG :GAUSS  ; gain-bandwidth product
+ TN         = 25                              ; nominal temperature in [C]
+ TMIN       = -55                             ; minimum temperature of operation (simulation) in [C]
+ TMAX       = 125                             ; maximum temperature of operation (simulation) in [C]
+ SR         = 0.5   STAT 0.3   0.7    :GAUSS  ; slew rate in [V/us]
+ VCC        = 15                              ; typical positive supply voltage
+ VIN_CM_MAX = 14.5  STAT 14.3  14.6   :GAUSS  ; higher limit of input common mode voltage
+ VEE        = -15                             ; typical negative supply voltage
+ VIN_CM_MIN = -12.5 STAT -12.6 -12.3  :GAUSS  ; lower limit of input common mode voltage
+ IBIAS_N    = 80N   STAT 40N   200N   :GAUSS  ; input bias current for nominal temperature
+ ROUT       = 200                             ; output resistance
+ ROAC       = 75    STAT 50    100    :GAUSS  ; output resistance for alternating current
+ AVD        = 200K  STAT 50K   500K   :GAUSS  ; open loop voltage gain in [V/V]
+ IOSN       = 20N   STAT -200N 200N   :GAUSS  ; input offset current for nominal temperature
+ IOSMIN     = 20N   STAT -200N 200N   :GAUSS  ; input offset current for minimum temperature
+ IOSMAX     = 7N    STAT -70N  70N    :GAUSS  ; input offset current for maximum temperature
+ VOSN       = 1M    STAT -5M   5M     :GAUSS  ; input offset voltage for nominal temperature
+ VOSMIN     = .0018 STAT -5M   5M     :GAUSS  ; input offset voltage for minimum temperature
+ VOSMAX     = .0001 STAT -5M   5M     :GAUSS  ; input offset voltage for maximum temperature
+ CMRR       = 90    STAT 70    110    :GAUSS  ; common mode rejection ratio in [dB]
+ FCMRR      = 300   STAT 200   1K     :GAUSS  ; 3dB frequency of common mode rejection ratio
+ PSRR       = 90    STAT 77    110    :GAUSS  ; average power supply rejection ratio in [dB]
+ FPSRRP     = 250   STAT 100   1K     :GAUSS  ; 3dB frequency of positive power supply rejection ratio
+ FPSRRN     = 250   STAT 100   1K     :GAUSS  ; 3dB frequency of negative power supply rejection ratio
+ ISUP       = .0015                           ; positive supply current
+ ISUP0      = .0012                           ; extrapolated supply current for VCC=0
+ OVERSHOOT  = 5                               ; overshoot of small signal pulse response in []
+ PHIM       = 50                              ; phase margin for 0dB frequency in [deg]
+ PHIM_OVRSHT= 0                               ; phase margin/overshoot switch [1/0]
+ ISCP       = 33M   STAT 40M   20M    :GAUSS  ; output positive short-circuit current
+ ISCN       = 21M   STAT 35M   15M    :GAUSS  ; output negative short-circuit current
+ VOUT_MIN   = -12.4 STAT -12.5 -12.2  :GAUSS  ; minimum output voltage
+ VOUT_MAX   = 13.8  STAT 13.9  13.6   :GAUSS  ; maximum output voltage
