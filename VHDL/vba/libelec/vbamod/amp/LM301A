+ GBW        = 3MEG   ; gain-bandwidth product
+ TN         = 25     ; nominal temperature in [C]
+ TMIN       = 0      ; minimum temperature of operation (simulation) in [C]
+ TMAX       = 70     ; maximum temperature of operation (simulation) in [C]
+ SR         = 10     ; slew rate in [V/us]
+ VCC        = 15     ; typical positive supply voltage
+ VIN_CM_MAX = 14.9   ; higher limit of input common mode voltage
+ VEE        = -15    ; typical negative supply voltage
+ VIN_CM_MIN = -12.5  ; lower limit of input common mode voltage
+ IBIAS_N    = 70E-9  ; input bias current for nominal temperature
+ IBIAS_MIN  = 77E-9  ; input bias current for minimum temperature
+ ROUT       = 100    ; output resistance
+ ROAC       = 50     ; output resistance for alternating current
+ AVD        = 160K   ; open loop voltage gain in [V/V]
+ IOSN       = 32E-10 ; input offset current for nominal temperature
+ IOSMIN     = 35E-10 ; input offset current for minimum temperature
+ IOSMAX     = 28E-10 ; input offset current for maximum temperature
+ VOSN       = .003   ; input offset voltage for nominal temperature
+ VOSMIN     = .0032  ; input offset voltage for minimum temperature
+ VOSMAX     = .0027  ; input offset voltage for maximum temperature
+ CMRR       = 100    ; common mode rejection ratio in [dB]
+ FCMRR      = 100    ; 3dB frequency of common mode rejection ratio
+ PSRR       = 105    ; average power supply rejection ratio in [dB]
+ FPSRRP     = 800    ; 3dB frequency of positive power supply rejection ratio
+ FPSRRN     = 10     ; 3dB frequency of negative power supply rejection ratio
+ ISUP       = .00175 ; positive supply current
+ ISUP0      = .00125 ; extrapolated supply current for VCC=0
+ OVRSHT     = 25     ; overshoot of small signal pulse response in []
+ PHIM       = 30     ; phase margin for 0dB frequency in [deg]
+ PHIM_OVRSHT= 0      ; phase margin/overshoot switch [1/0]
+ ISCP       = 0.027  ; output positive short-circuit current
+ ISCN       = 0.027  ; output negative short-circuit current
+ VOUT_MIN   = -12.5  ; minimum output voltage
+ VOUT_MAX   = 12.5   ; maximum output voltage
