.PARSET LF347 _OPAMPFET
+ VNOISE     = 20     ; spectral noise density in [nV/sqrt(Hz)]
+ GBW        = 4000K  ; gain-bandwidth product
+ TN         = 25     ; nominal temperature in [C]
+ TMIN       = 0      ; minimum temperature of operation (simulation) in [C]
+ TMAX       = 70     ; maximum temperature of operation (simulation) in [C]
+ SRP        = 12.8   ; positive going slew rate for nominal temperature in [V/us]
+ SRNN       = 13     ; negative going slew rate for nominal temperature in [V/us]
+ SRNMIN     = 13     ; negative going slew rate for min temperature in [V/us]
+ SRNMAX     = 13     ; negative going slew rate for max temperature in [V/us]
+ VCC        = 15     ; typical positive supply voltage
+ VIN_CM_MAX = 14.9   ; higher limit of input common mode voltage
+ VEE        = -15    ; typical negative supply voltage
+ VIN_CM_MIN = -11.9  ; lower limit of input common mode voltage
+ IBIAS_N    = 50E-12 ; input bias current for nominal temperature
+ IBIAS_MAX  = 5E-10  ; input bias current for maximum temperature
+ ROUT       = 200    ; output resistance
+ ROAC       = 50     ; output resistance for alternating current
+ AVD        = 100K   ; open loop voltage gain in [V/V]
+ IOSN       = 25E-12 ; input offset current for nominal temperature
+ IOSMAX     = 25E-11 ; input offset current for maximum temperature
+ VOSN       = .005   ; input offset voltage for nominal temperature
+ VOSMIN     = .0052  ; input offset voltage for minimum temperature
+ VOSMAX     = .0046  ; input offset voltage for maximum temperature
+ CMRR       = 100    ; common mode rejection ratio in [dB]
+ FCMRR      = 500    ; 3dB frequency of common mode rejection ratio
+ PSRR       = 100    ; average power supply rejection ratio in [dB]
+ FPSRRP     = 300    ; 3dB frequency of positive power supply rejection ratio
+ FPSRRN     = 9      ; 3dB frequency of negative power supply rejection ratio
+ ISUP       = .002   ; supply current
+ ISUP0      = .0018  ; extrapolated supply current for VCC=0
+ OVERSHOOT  = 20     ; overshoot of small signal pulse response in []
+ PHIM       = 40     ; phase margin for 0dB frequency in [deg]
+ PHIM_OVRSHT= 0      ; phase margin/overshoot switch [1/0]
+ ISCP       = .03    ; output positive short-circuit current
+ ISCN       = .03    ; output negative short-circuit current
+ VOUT_MIN   = -13    ; minimum output voltage
+ VOUT_MAX   = 13     ; maximum output voltage
