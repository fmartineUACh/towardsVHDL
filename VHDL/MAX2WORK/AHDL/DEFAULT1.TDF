SUBDESIGN default1
(
	i[3..0]				: INPUT;
	ascii_code[7..0]	: OUTPUT;
)
BEGIN
	DEFAULTS
		ascii_code[] = B"00111111";	% "?" %
	END DEFAULTS;

	TABLE
		i[3..0]	=>	ascii_code[];

		B"1000"	=>	B"01100001";	% "a" %
		B"0100"	=>	B"01100010";	% "b" %
		B"0010"	=>	B"01100011";	% "c" %
		B"0001"	=>	B"01100100";	% "d" %
	END TABLE;
END;
