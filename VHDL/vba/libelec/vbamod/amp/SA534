+ VNOISE     = 40     ; spectral noise density in [nV/sqrt(Hz)]
+ GBW        = 1MEG   ; gain-bandwidth product
+ TN         = 25     ; nominal temperature in [C]
+ TMIN       = -40    ; minimum temperature of operation (simulation) in [C]
+ TMAX       = 85     ; maximum temperature of operation (simulation) in [C]
+ SR         = 0.3    ; slew rate in [V/us]
+ VCC        = 5      ; typical positive supply voltage
+ VIN_CM_MAX = 4.1    ; higher limit of input common mode voltage
+ VEE        = 0      ; typical negative supply voltage
+ VIN_CM_MIN = 0      ; lower limit of input common mode voltage
+ IBIAS_N    = 25E-9  ; input bias current for nominal temperature
+ IBIAS_MIN  = 24E-9  ; input bias current for minimum temperature
+ ROUT       = 200    ; output resistance
+ ROAC       = 75     ; output resistance for alternating current
+ AVD        = 100K   ; open loop voltage gain in [V/V]
+ IOSN       = 5E-9   ; input offset current for nominal temperature
+ IOSMIN     = 5.5E-9 ; input offset current for minimum temperature
+ IOSMAX     = 4.5E-9 ; input offset current for maximum temperature
+ VOSN       = .002   ; input offset voltage for nominal temperature
+ VOSMIN     = .0024  ; input offset voltage for minimum temperature
+ VOSMAX     = .0015  ; input offset voltage for maximum temperature
+ CMRR       = 100    ; common mode rejection ratio in [dB]
+ FCMRR      = 120000 ; 3dB frequency of common mode rejection ratio
+ PSRR       = 100    ; average power supply rejection ratio in [dB]
+ FPSRRP     = 140000 ; 3dB frequency of positive power supply rejection ratio
+ FPSRRN     = 140000 ; 3dB frequency of negative power supply rejection ratio
+ ISUP       = .0002  ; supply current
+ ISUP0      = .00015 ; extrapolated supply current for VCC=0
+ OVERSHOOT  = 20     ; overshoot of small signal pulse response in []
+ PHIM       = 40     ; phase margin for 0dB frequency in [deg]
+ PHIM_OVRSHT= 0      ; phase margin/overshoot switch [1/0]
+ ISCP       = 0.04   ; output positive short-circuit current
+ ISCN       = 0.04   ; output negative short-circuit current
+ VOUT_MIN   = 1.1    ; minimum output voltage
+ VOUT_MAX   = 3.4    ; maximum output voltage
